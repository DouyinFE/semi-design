{
    "name": "@douyinfe/semi-foundation",
    "version": "2.90.5",
    "description": "",
    "scripts": {
        "clean": "rimraf lib",
        "build:lib": "node ./scripts/compileLib.js",
        "prepublishOnly": "npm run clean && npm run build:lib"
    },
    "files": [
        "lib/*",
        "**/*.ts",
        "**/*.scss",
        "!**/node_modules/**",
        "!**/*.test.ts",
        "!**/*.spec.ts"
    ],
    "main": "lib/cjs/base/index.js",
    "module": "lib/es/base/index.js",
    "types": "lib/cjs/base/index.d.ts",
    "typings": "lib/cjs/base/index.d.ts",
    "typesVersions": {
        "*": {
            "lib/es/*": [
                "lib/es/*",
                "lib/es/*/index"
            ],
            "lib/es/*/*": [
                "lib/es/*/*",
                "lib/es/*/*/index"
            ],
            "lib/es/*/*/*": [
                "lib/es/*/*/*",
                "lib/es/*/*/*/index"
            ],
            "lib/es/*/*/*/*": [
                "lib/es/*/*/*/*",
                "lib/es/*/*/*/*/index"
            ],
            "lib/cjs/*": [
                "lib/cjs/*",
                "lib/cjs/*/index"
            ],
            "lib/cjs/*/*": [
                "lib/cjs/*/*",
                "lib/cjs/*/*/index"
            ],
            "lib/cjs/*/*/*": [
                "lib/cjs/*/*/*",
                "lib/cjs/*/*/*/index"
            ],
            "lib/cjs/*/*/*/*": [
                "lib/cjs/*/*/*/*",
                "lib/cjs/*/*/*/*/index"
            ]
        }
    },
    "exports": {
        ".": {
            "types": "./lib/es/base/index.d.ts",
            "import": "./lib/es/base/index.js",
            "require": "./lib/cjs/base/index.js"
        },
        "./*": {
            "types": "./lib/es/*/index.d.ts",
            "import": "./lib/es/*/index.js",
            "require": "./lib/cjs/*/index.js"
        },
        "./lib/es/*/*.css": {
            "import": "./lib/es/*/*.css",
            "require": "./lib/cjs/*/*.css",
            "default": "./lib/es/*/*.css"
        },
        "./lib/es/*.css": {
            "import": "./lib/es/*.css",
            "require": "./lib/cjs/*.css",
            "default": "./lib/es/*.css"
        },
        "./lib/cjs/*/*.css": {
            "import": "./lib/es/*/*.css",
            "require": "./lib/cjs/*/*.css",
            "default": "./lib/cjs/*/*.css"
        },
        "./lib/cjs/*.css": {
            "import": "./lib/es/*.css",
            "require": "./lib/cjs/*.css",
            "default": "./lib/cjs/*.css"
        },
        "./lib/es/*": {
            "types": [
                "./lib/es/*.d.ts",
                "./lib/es/*/index.d.ts"
            ],
            "import": "./lib/es/*",
            "default": "./lib/es/*"
        },
        "./lib/es/*/*": {
            "types": [
                "./lib/es/*/*.d.ts",
                "./lib/es/*/*/index.d.ts"
            ],
            "import": "./lib/es/*/*",
            "default": "./lib/es/*/*"
        },
        "./lib/es/*/*/*": {
            "types": [
                "./lib/es/*/*/*.d.ts",
                "./lib/es/*/*/*/index.d.ts"
            ],
            "import": "./lib/es/*/*/*",
            "default": "./lib/es/*/*/*"
        },
        "./lib/es/*/*/*/*": {
            "types": [
                "./lib/es/*/*/*/*.d.ts",
                "./lib/es/*/*/*/*/index.d.ts"
            ],
            "import": "./lib/es/*/*/*/*",
            "default": "./lib/es/*/*/*/*"
        },
        "./lib/cjs/*": {
            "types": [
                "./lib/cjs/*.d.ts",
                "./lib/cjs/*/index.d.ts"
            ],
            "require": "./lib/cjs/*",
            "default": "./lib/cjs/*"
        },
        "./lib/cjs/*/*": {
            "types": [
                "./lib/cjs/*/*.d.ts",
                "./lib/cjs/*/*/index.d.ts"
            ],
            "require": "./lib/cjs/*/*",
            "default": "./lib/cjs/*/*"
        },
        "./lib/cjs/*/*/*": {
            "types": [
                "./lib/cjs/*/*/*.d.ts",
                "./lib/cjs/*/*/*/index.d.ts"
            ],
            "require": "./lib/cjs/*/*/*",
            "default": "./lib/cjs/*/*/*"
        },
        "./lib/cjs/*/*/*/*": {
            "types": [
                "./lib/cjs/*/*/*/*.d.ts",
                "./lib/cjs/*/*/*/*/index.d.ts"
            ],
            "require": "./lib/cjs/*/*/*/*",
            "default": "./lib/cjs/*/*/*/*"
        }
    },
    "dependencies": {
        "@douyinfe/semi-animation": "2.90.5",
        "@douyinfe/semi-json-viewer-core": "2.90.5",
        "@mdx-js/mdx": "^3.0.1",
        "async-validator": "^3.5.0",
        "classnames": "^2.2.6",
        "date-fns": "^2.29.3",
        "date-fns-tz": "^1.3.8",
        "fast-copy": "^3.0.1 ",
        "lodash": "^4.17.21",
        "lottie-web": "^5.12.2",
        "memoize-one": "^5.2.1",
        "prismjs": "^1.29.0",
        "remark-gfm": "^4.0.0",
        "scroll-into-view-if-needed": "^2.2.24"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "sideEffects": [
        "*.scss",
        "*.css"
    ],
    "gitHead": "208186435ee571d1dde07a7fa378050d56d94fe2",
    "devDependencies": {
        "@babel/plugin-transform-runtime": "^7.15.8",
        "@babel/preset-env": "^7.15.8",
        "@types/lodash": "^4.14.176",
        "@types/mdx": "^2.0.13",
        "@types/prismjs": "^1.26.3",
        "babel-plugin-lodash": "^3.3.4",
        "del": "^6.0.0",
        "esbuild": "0.24.0",
        "gulp": "^4.0.2",
        "gulp-babel": "^8.0.0",
        "gulp-replace": "^1.1.3",
        "gulp-sass": "^5.0.0",
        "gulp-typescript": "^6.0.0-alpha.1",
        "merge2": "^1.4.1",
        "rimraf": "^3.0.2",
        "through2": "^4.0.2"
    }
}
